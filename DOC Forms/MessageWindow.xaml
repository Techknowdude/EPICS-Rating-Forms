<Window x:Class="DOC_Forms.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DOC_Forms"
        mc:Ignorable="d"
        Title="Alert" Height="129.184" Width="251.633" WindowStartupLocation="CenterOwner" ShowInTaskbar="False" ResizeMode="NoResize">
    <Grid>
        <Label HorizontalAlignment="Center" MaxWidth="200" MaxHeight="100" VerticalAlignment="Center" Content="{Binding Message}" />
    </Grid>
</Window>
